// generally we loop over 10 addresses in ram and double values there

movl reg0 0     // address
movli reg0 10   // counter

inc reg0     // Loop
load reg1 reg0 


mov reg2 reg1           
add reg1 reg2 reg1

store reg1 reg0

deci reg0
movli reg3 2    // instruction number
jne reg3         // jump if reg0 == 0




